#include <stdio.h>
#include <stdlib.h>

void funcao(int n)
{
    int i, j;
    for (i=1; i<=n; i++)
    {
        for(j=1; j<=i; j++)
        {
            printf("%d", j);
            if (j<i)
                printf(" ");
        }
        if (i<n)
            printf("\n");
    }
}

int main()
{
    int x;
    scanf("%d", &x);
    funcao(x);
    return 0;
}
