#include <stdio.h>
#include <stdlib.h>

int main()
{
    double a, b, c;
    scanf ("%lf%lf%lf", &a, &b, &c);

    if (a<b && a<c)
    {
       printf ("Primeiro produto\n");
    }
    if (b<a && b<c)
    {
        printf ("Segundo produto\n");

    }
    if (c<a && c<b)
    {
        printf ("Terceiro produto\n");

    }

    return 0;
}
