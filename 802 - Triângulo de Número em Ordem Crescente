#include <stdio.h>
#include <stdlib.h>
/*( 1<= n <= 30 )*/


int main()
{
    int n, m, count;
    scanf("%d", &n);

    for(count=1;count<=n;count++)//linhas. come�a com 1 e vai at� o n�mero digitado. Ent�o n=4=4 linhas
    {
        for(m=1;m<=count;m++)//n� de cada linha
            printf("%d", count);

        printf("\n");//para n�o ficarem todos a mesma linha ou todos separados em cada linha. Dentro do 1� for
    }

    return 0;
}
