#include <stdio.h>

int triangulo(int x);

int main()
{
    int n;
    scanf("%d", &n);
    triangulo(n);
    return 0;
}

int triangulo(int x)
{
    int i, j;
    for (i=1; i<=x; i++)
    {
        for (j=1; j<=i; j++)
        {
            if(j>1)
                 printf("-");
            printf("%d", i);
        }
       printf("\n");
    }
    return 0;
}
