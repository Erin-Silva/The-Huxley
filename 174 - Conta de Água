#include <stdio.h>
#include <stdlib.h>

int main()
{
    int N, valor;
    scanf ("%d", &N);

    if (N<=10)
    {
       valor = 7;
    }
    else if (N<=30)
    {
        valor = 7+(N-10)*1;
    }
    else if (N<=100)
    {
        valor = 7+20+(N-30)*2;
    }
    else
    {
        valor = 7+20+140+(N-100)*5;
    }
    printf ("%d\n", valor);

    return 0;
}
