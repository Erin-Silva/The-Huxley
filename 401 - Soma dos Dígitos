#include <stdio.h>
#include <stdlib.h>


int SomaDigitos(int n)
{
    int soma=0;
    while (n>0)
    {
        soma+=n%10;
        n=n/10;
    }
    return soma;
}

int main()
{
    int n, somaMain;
    scanf("%d", &n);

    somaMain = SomaDigitos(n);
    printf("%d\n", somaMain);

    return 0;
}
