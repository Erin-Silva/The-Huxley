#include <stdio.h>
#include <stdlib.h>

int main()
{
    long long int N;
    scanf ("%li", &N);

    if (N==0)
    {
       printf ("NULO\n");
    }
    else
    {


    if (N>0)
    {
        printf("POSITIVO ");
    }
    else
    {
        printf("NEGATIVO ");
    }

    if (N%2==0)
    {
        printf("PAR\n");
    }
    else
    {
        printf ("IMPAR\n");
    }

    }

    return 0;
}
