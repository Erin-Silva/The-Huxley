#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int ano;
    scanf ("%d", &ano);
    
    if ((ano%400==0) || (ano%4==0) && (ano%100!=0))
    {
        printf ("BISSEXTO\n");
    }
    else
    {
        printf ("NAOBISSEXTO\n");
    }
    
	return 0;
}
