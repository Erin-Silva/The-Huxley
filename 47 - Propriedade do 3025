#include <stdio.h>
#include <stdlib.h>
/*valor menor que 1000 ou maior que 9999.*/

int main()
{
    int n;
    scanf("%d", &n);

    while(n>=1000 && n<=9999)
    {

        if(n==((n/100)+(n%100))*((n/100)+(n%100)))
           printf("propriedade do 3025!\n");
        else
           printf("numero comum\n");
           scanf("%d", &n);
    }

    return 0;
}
